    "es5-shimify": "0.0.1",
    "hem": "0.1.9",
    "jqueryify": "1.8.0",
    "json2ify": "0.0.1",
    "moment": "1.7.2",
    "nib": "0.8.2",
    "publisssh": "0.1.3",
    "serveup": "~0.0.5",
